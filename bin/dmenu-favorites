#!/bin/sh

x=$(echo -e "browser\nfiles\nemacs\ngvim\nhtop\nvirtualbox\ntabbed-st" | dmenu -i -p "Favorites")
case "$x" in
    browser) qutebrowser & disown ;;
    files) st ranger & disown ;;
    emacs) emacs & disown ;;
    gvim) gvim & disown ;;
    htop) st htop & disown ;;
    virtualbox) virtualbox & disown ;;
    tabbed-st) tabbed -r 2 st -w '' ;;
esac
