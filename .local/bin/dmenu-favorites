#!/bin/sh
x=$(echo -e "browser\nfiles\nemacs\nhtop\nvirtualbox\nsurf" | dmenu -i -p "Favorites")

case "$x" in
    browser) qutebrowser & disown ;;
    files) st ranger & disown ;;
    emacs) emacs & disown ;;
    htop) st htop & disown ;;
    virtualbox) virtualbox & disown ;;
    surf) surf-open ;;
esac
