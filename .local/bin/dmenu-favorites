#!/bin/sh
x=$(echo -e "browser\nfiles\ntmux\nhtop\nvirtualbox\neplayer" | dmenu -i -p "Favorites")

case "$x" in
    browser) qutebrowser & disown ;;
    files) st ranger & disown ;;
    eplayer) electronplayer & disown ;;
    htop) st htop & disown ;;
    virtualbox) virtualbox & disown ;;
    tmux) st -e tmux a -t $USER;
esac
